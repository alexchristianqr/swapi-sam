{
  "name": "template-project-aws",
  "version": "1.0.0",
  "description": "Plantilla de trabajao para AWS",
  "author": "Alex Christian",
  "license": "MIT",
  "private": true,
  "scripts": {
    "compile": "tsc",
    "build": "node build-template.js --template=example && sam build --config-file samconfig-develop.toml",
    "deploy": "sam deploy --config-file samconfig-develop.toml",
    "aws": "npm run build && npm run deploy",
    "serve": "node src/app/main.js",
    "pm2:start": "pm2 start src/app/main.js",
    "nodemon:start": "nodemon start src/app/main.js"
  },
  "dependencies": {
    "@aws-sdk/client-eventbridge": "^3.194.0",
    "@aws-sdk/client-sfn": "^3.262.0",
    "amazon-cognito-identity-js": "^6.2.0",
    "aws-sdk": "^2.1440.0",
    "axios": "^1.1.3",
    "cors": "^2.8.5",
    "dotenv": "6.1.0",
    "express": "~4.16.1",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.0",
    "serverless-iam-roles-per-function": "^2.0.2",
    "serverless-step-functions": "^2.10.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.119",
    "@types/node": "^20.5.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.22",
    "pm2": "^5.3.0",
    "prettier": "^2.8.4",
    "typescript": "^5.1.6"
  }
}
