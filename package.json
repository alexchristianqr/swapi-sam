{
  "name": "template-project-aws",
  "version": "1.0.0",
  "description": "Plantilla de trabajo para AWS",
  "author": "Alex Christian",
  "license": "MIT",
  "private": true,
  "scripts": {
    "aws:build": "node sam build --config-file samconfig-develop.toml",
    "aws:deploy": "sam deploy --config-file samconfig-develop.toml",
    "aws": "npm run aws:build && npm run aws:deploy",
    "build": "tsc",
    "serve": "node src/app/main.js",
    "pm2:start": "pm2 start src/app/main.js",
    "nodemon:start": "npm run build && nodemon start ./dist/app.js"
  },
  "dependencies": {
    "aws-sdk": "^2.1440.0",
    "axios": "^1.1.3",
    "cors": "^2.8.5",
    "dotenv": "6.1.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.119",
    "@types/node": "^20.5.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.4",
    "typescript": "^5.1.6"
  }
}
